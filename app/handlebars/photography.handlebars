<!DOCTYPE html>

<head>
  {{> head}}
</head>

<body class='photography-page'>
  {{> slide_in_menu}}
  {{> top_menu}}

  <header>
    <div class='page-tracker'>
      <h4>{{pageTitle}}</h4>
      <p class='photo-category'>Photography</p>
    </div>

    <button class='menu-open menu-btn'>
      <img src='./img/menu-closed.svg' alt='menu-btn' />
    </button>
  </header>

  <section class='photography-section page-section'>
    <div class='container'>
      <img src='./img/photography/lifestyle-hot8-2.jpg' class='photography-image vert' data-page="occasions" />
      <img src='./img/photography/lifestyle-hot8-1.jpg' class='photography-image ' data-page="occasions" />
      <img src='./img/photography/lifestyle-hot8-3.jpg' class='photography-image vert' data-page="movement" />
      <img src='./img/photography/lifestyle-hot8-4.jpg' class='photography-image vert' data-page="movement" />
      <img src='./img/photography/lifestyle-hot8-5.jpg' class='photography-image vert' data-page="lifestyle" />
      <img src='./img/photography/lifestyle-hot8-6.jpg' class='photography-image ' data-page="lifestyle" />
      <img src='./img/photography/lifestyle-hot8-7.jpg' class='photography-image vert' data-page="portraits" />
      <img src='./img/photography/lifestyle-hot8-8.jpg' class='photography-image ' data-page="portraits" />
      <img src='./img/photography/lifestyle-hot8-9.jpg' class='photography-image vert' data-page="movement" />
      <img src='./img/photography/lifestyle-hot8-10.jpg' class='photography-image vert' data-page="lifestyle" />
      <img src='./img/photography/lifestyle-hot8-11.jpg' class='photography-image ' data-page="portraits" />
      <img src='./img/photography/lifestyle-hot8-2.jpg' class='photography-image vert' data-page="occasions" />
      <img src='./img/photography/lifestyle-hot8-1.jpg' class='photography-image ' data-page="occasions" />
      <img src='./img/photography/lifestyle-hot8-3.jpg' class='photography-image vert' data-page="movement" />
      <img src='./img/photography/lifestyle-hot8-4.jpg' class='photography-image vert' data-page="movement" />
      <img src='./img/photography/lifestyle-hot8-5.jpg' class='photography-image vert' data-page="lifestyle" />
      <img src='./img/photography/lifestyle-hot8-6.jpg' class='photography-image ' data-page="lifestyle" />
      <img src='./img/photography/lifestyle-hot8-7.jpg' class='photography-image vert' data-page="portraits" />
      <img src='./img/photography/lifestyle-hot8-8.jpg' class='photography-image ' data-page="portraits" />
      <img src='./img/photography/lifestyle-hot8-9.jpg' class='photography-image vert' data-page="movement" />
      <img src='./img/photography/lifestyle-hot8-10.jpg' class='photography-image vert' data-page="lifestyle" />
      <img src='./img/photography/lifestyle-hot8-11.jpg' class='photography-image ' data-page="portraits" />
    </div>
  </section>

  <footer>
    <div class='social-links'>
      <a href='mailto:{{instagram}}' class='footer-icon'>
        <img src='./img/mail-gray.svg' class='email ' alt='email' />
      </a>
      <a href='https://www.instagram.com/{{instagram}}/' class='footer-icon'>
        <img src='./img/instagram-gray.svg' class='instagram' alt='instagram' />
      </a>
    </div>
  </footer>
  {{> js}}
</body>

</html>